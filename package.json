{
    "name": "tfm-2020",
    "engines": {
        "node": "16.x"
    },
    "scripts": {
        "dev": "next",
        "build": "next build",
        "start": "next start -H 0.0.0.0",
        "prettier:base": "prettier --parser typescript",
        "prettier:check": "npm run prettier:base -- --list-different \"**/*.{ts,tsx}\"",
        "prettier:write": "npm run organize-imports && npm run prettier:base -- --write \"**/*.{js,ts,tsx}\" \"!.next\"",
        "organize-imports": "organize-imports-cli tsconfig.json",
        "storybook": "start-storybook -s ./assets -p 6006",
        "build-storybook": "build-storybook",
        "postinstall": "patch-package"
    },
    "overrides": {
        "nextjs-jsoneditor": {
            "react": "$react"
        }
    },
    "dependencies": {
        "bcryptjs": "2.4.3",
        "color": "4.2.0",
        "cookie": "0.4.1",
        "immer": "9.0.12",
        "jsoneditor": "^9.10.0",
        "jsonwebtoken": "^9.0.0",
        "mailgun.js": "^8.2.1",
        "mongoose": "^6.10.5",
        "next": "^12.3.4",
        "nextjs-jsoneditor": "^0.0.2",
        "react": "18.2.0",
        "react-laag": "2.0.5",
        "react-redux": "8.0.5",
        "react-responsive-masonry": "2.1.7",
        "react-scrollable-feed": "1.3.2",
        "react-tippy": "1.4.0",
        "react-toastify": "8.1.0",
        "react-twemoji": "0.5.0",
