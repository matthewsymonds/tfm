        "@babel/preset-typescript": "7.16.7",
        "@storybook/react": "^7.0.4",
        "@types/babel__core": "7.20.0",
        "@types/bcryptjs": "2.4.2",
        "@types/color": "3.0.2",
        "@types/cookie": "0.4.1",
        "@types/eslint": "8.37.0",
        "@types/eslint-plugin-prettier": "3.1.0",
        "@types/jsonwebtoken": "8.5.1",
        "@types/mailgun-js": "0.22.0",
        "@types/node": "17.0.8",
        "@types/prettier": "2.7.2",
