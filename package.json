        "babel-loader": "8.2.3",
        "babel-plugin-styled-components": "2.0.2",
        "eslint": "8.8.0",
        "eslint-config-prettier": "8.3.0",
        "eslint-plugin-prettier": "4.0.0",
        "organize-imports-cli": "0.9.0",
        "patch-package": "6.4.7",
        "postcss": "^8.4.21",
        "prettier": "2.5.1",
